#CALCULADORA
# Esta calculadora realiza el calculo del area lateral de un prisma

# Declaracion de la variable
perimetro_base,altura,area_lateral_prisma=0.0,0.0,0.0

# Calculadora
prerimetro_base= 120
altura=13
area_lateral_prisma=perimetro_base*altura

#mostrar datos
print("prerimetro_base = ", prerimetro_base)
print("altura = ", altura)
print("area_lateral_prisma = ", area_lateral_prisma)

#verificador
menor_que=(area_lateral_prisma>10)
print("el prisma tiene un area lateral menor a 10 m**2?", menor_que)

